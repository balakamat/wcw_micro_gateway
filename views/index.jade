doctype html
html(lang='en', ng-app='MyApp')
	head
		title Wipro Connected Warehouse
		script(src='js/jquery/jquery.min.js')
		script(src='js/angular/angular.js')
		script(src='js/angular/angular-route.js')
		script(src='js/angular/angular-resource.min.js')
		script(src='js/angular/angular-cookies.min.js')
		script(src='js/angular/angular-sanitize.js')
		script(src='bower_components/d3/d3.js')
		script(src='bower_components/nvd3/nv.d3.js')
		script(src='bower_components/angularjs-nvd3-directives/dist/angularjs-nvd3-directives.js')
		script(src='bower_components/angular-nvd3-master/dist/angular-nvd3.js')
		script(src='bower_components/highcharts-ng-master/dist/highcharts-ng.js')
		script(src='bower_components/highcharts-ng-master/dist/highcharts.js')
		script(src='http://code.highcharts.com/highcharts-3d.js')
		script(src='js/bootstrap/bootstrap.min.js')
		script(src='js/jquery/jquery-1.7.1.js')
		script(src='js/jquery/jquery-ui.js')
		script(src='js/massautocomplete.js')
		script(src='js/app.js')
		script(src='js/services.js')
		script(src='js/directives.js')
		script(src='js/controllers.js')
		script(src='js/HomeController.js')
		script(src='js/angular/angular-animate.js')
		script(src='js/bootstrap/ui-bootstrap-tpls-0.12.0.js')
		script(src='bower_components/nvd3/angularjs-nvd3-directives.js')
		script(src='../../bower_components/nvd3/components/d3/d3.js')
		script(src='../../bower_components/nvd3/nvd3/nv.d3.js')
		script(src='bower_components/angular-socket-io/socket.js')
		script(src='js/jquery/jquery.btechco.excelexport.js')
		script(src='js/jquery/jquery.base64.js')				
		link(rel='stylesheet', href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css")
		link(rel='stylesheet', href='http://ajax.googleapis.com/ajax/libs/jqueryui/1/themes/flick/jquery-ui.css')
		link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
		link(rel='stylesheet', href='bower_components/nvd3/nv.d3.css')
		link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.css')

		link(rel='stylesheet', href='/stylesheets/style.css')
		link(rel='stylesheet', href='/stylesheets/style3.css')
		link(rel='stylesheet', href='/stylesheets/style2.css')
		link(rel='stylesheet', href='/stylesheets/nav.css')

		
		link(rel='images', href='/images/coming.png')
		
	body(ng-controller="MainController")
	div
		
		div.right-block
			div.nav.navbar.top-menu
				div.navbar-header
					div(style="width:43px;font-size: 1.4em;", ng-mouseover='showNavbar()') 
						img(src='stylesheets/images/warehouselogo.png', width='40', align="left", style='margin-top:10px')
					div(style="width:120px;margin-left:10px")
						p.logostyle Connected <br/> Warehouse
					div(style="width:30%;height:60px;text-align: left;position:relative:float:right")
						p.page-title &nbsp &nbsp;{{pageTitle}}
					
					div.glystyle
						a(href='#/home')
							span.span-style.glyphicon.glyphicon-home
						
						a(href='#/Dashboard/Alerts',style='position:relative;')
							span.badge.badge-notify {{openalerts}}
							span.glyphicon.glyphicon-bell.span-style
							
						span.span-style.glyphicon.glyphicon-search
						
						span(style='position: relative;display: inline-block;margin-top:0;', ng-mouseover='showCogMenu=true;', ng-mouseout='showCogMenu=false;')
							span.span-style.glyphicon.glyphicon-cog
							ul(style='background-color: rgb(46,117,182);position: absolute;left: 0;list-style-type:none;color: #FFF;', ng-show='showCogMenu')
								li(style='border-bottom:2px solid #FFF;width:100px;')
									a(href='#/My Dashboard') My Dashboard
								li(style='border-bottom:2px solid #FFF')
									a(href='asn.html') Admin
								li
									a(href='asn.html') Defaults
								
						span.span-style.glyphicon.glyphicon-question-sign
					
						a(href='/login')
							span.span-style-logout.glyphicon.glyphicon-log-out
						span
							p.user User : {{user.name}}
						
						
						
					div(style="width:60px;")
						img.logo(src='stylesheets/images/WCCB_Logo.png', alt='Smiley face', height='64', width='64')

        

										
			.right-content(ng-view='',ng-mouseover = 'hideNavbar()')
		.left-menu 
			.left-nav(ng-controller='MenuController')
				.site-nav
								
					ul
						li(ng-repeat='menu in tutorialmenu', ng-click='showHideSubmenu(menu)', ng-show="menu.showmenu")
								a(ng-click='hideTutorials()', href='/#/{{menu.label}}') {{menu.label}}
					div(ng-if='selectedMenu.submenu && selectedMenu.submenu.length', data-menu='{{selectedMenu.label}}', ng-show='selectedMenu.showsubmenu')
						ul(class="")
							li(ng-repeat='submenu in selectedMenu.submenu')
								a(ng-click='hideTutorials();changeTitle(submenu)', href='/#/Dashboard/{{submenu}}') {{submenu}}
							li
								a(ng-click='backMenu()') 
									span.glyphicon.glyphicon-arrow-left
					div(ng-if='selectedMenu.lang && selectedMenu.lang.length', data-menu='{{selectedMenu.label}}', ng-show='selectedMenu.showsubmenu')
						ul(class="")
							li(ng-repeat='lang in selectedMenu.lang')
								a(ng-click='hideTutorials()', href='/#/Dashboard/{{lang.label}}'){{lang.label}}
							li
								a(ng-click='backMenu()') 
									span.glyphicon.glyphicon-arrow-left
					div(ng-if='selectedMenu.searchmenu && selectedMenu.searchmenu.length', data-menu='{{selectedMenu.label}}', ng-show='selectedMenu.showsubmenu')
						ul(class="")
							li(ng-repeat='searchmenu in selectedMenu.searchmenu')
								a(ng-click='hideTutorials();changeTitle(searchmenu)', href='/#/{{searchmenu.label}}'){{searchmenu.label}}	
						ul(class="")
							li(ng-repeat='opmenu in selectedMenu.opmenu')
								a(ng-click='hideTutorials();changeTitle(opmenu)', href='/#/{{opmenu.label}}'){{opmenu.label}}			
							li
								a(ng-click='backMenu()', href='/#/') 
									span.glyphicon.glyphicon-arrow-left
					